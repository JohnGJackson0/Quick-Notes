{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "reset": "rm -rf node_modules/ && watchman watch-del-all && npm i",
    "eject": "expo eject",
    "lint": "eslint . --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx",
    "test": "jest --verbose --coverage --testPathIgnorePatterns=/cypress/",
    "test:e2e": "npm run web && ps -ef | grep cypress && cypress open ",
    "test:e2e:open": "cypress open",
    "test:update": "jest --verbose --coverage --updateSnapshot --testPathIgnorePatterns=/cypress/",
    "test:watch": "jest --verbose --watch --onlyChanged --coverage --testPathIgnorePatterns=/cypress/",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run test"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.11",
    "@react-native-async-storage/async-storage": "^1.15.12",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/react-hooks": "^7.0.2",
    "expo": "~43.0.2",
    "expo-status-bar": "~1.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-background-task": "^0.2.1",
    "react-native-cn-quill": "^0.7.15",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.7.1",
    "react-native-htmlview": "^0.16.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modalize": "^2.0.12",
    "react-native-paper": "^4.10.0",
    "react-native-portalize": "^1.0.7",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.13.0",
    "react-onclickoutside": "^6.12.1",
    "react-redux": "^7.2.6",
    "react-test-renderer": "^17.0.2",
    "redux-persist": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-htmlview": "^0.12.3",
    "@types/react-onclickoutside": "^6.7.4",
    "@types/react-test-renderer": "^17.0.1",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "babel-preset-expo": "~8.3.0",
    "cypress": "^9.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-native-typescript": "^2.2.6",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-expo": "~43.0.0",
    "jest-native": "^3.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.5",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "private": true
}
